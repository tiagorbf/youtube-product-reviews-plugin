{
  "name": "Youtube İnceleme Butonu",
  "version": "1.0.10",
  "description": "Artık almak istediğiniz ürünün Youtube incelemelerine bir tuş ile ulaşabilirsiniz! Amazon, Trendyol, HepsiBurada ve gittigidiyor'u desteklemektedir.",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "include_globs": [
        "*://*.amazon.*/*",
        "*://*.thomann.*/*",
        "*://*.musicstore.*/*",
        "*://*.ebay.*/*",
        "*://*.etsy.*/*",
        "*://*.royalqueenseeds.*/*",
        "*://*.alibaba.*/*",
        "*://*.trendyol.*/*"
      ],
      "js": ["extensions/content.js"],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "browser_action": {
    "default_icon" : "icon.png"
  },
  "web_accessible_resources": ["icon.png", "youtube.png"],
  "icons" : {
    "32" : "icon.png",
    "48" : "icon_48.png"
  }
}
